nav.navbar.navbar-default.navbar-fixed-top(ng-controller="pageTemplateCtrl")
	.container
		// Brand and toggle get grouped for better mobile display
		.navbar-header.page-scroll
			button.navbar-toggle(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
				span.sr-only Toggle navigation
				span.icon-bar
				span.icon-bar
				span.icon-bar
			a.navbar-brand(href='#page-top') Start Bootstrap
		// Collect the nav links, forms, and other content for toggling
		#bs-example-navbar-collapse-1.collapse.navbar-collapse
			ul.nav.navbar-nav.navbar-right(ng-sortable="menusConfig")
				li.page-scroll(ng-repeat="menu in menus.hashPoints" sortable)
					a(href="{{menu.url}}" target="{{menu.target}}" ng-class="menu.classes" ng-click="handleClick($event, menu)") {{menu.title}}
				li.page-scroll.new-menu-item(ng-click="createMenuItem('hashPoints')" ng-if="editMode")
					a
						i.fa.fa-plus
						|  Add Menu
